{
  "name": "gomflow-discord",
  "version": "1.0.0",
  "description": "GOMFLOW Discord Bot Service - Interactive group order management through Discord with slash commands and embeds",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "test:smoke:staging": "npm run test:smoke -- --env=staging",
    "test:smoke:production": "npm run test:smoke -- --env=production",
    "test:smoke": "jest --testPathPattern=smoke",
    "deploy:commands": "tsx src/scripts/deployCommands.ts"
  },
  "dependencies": {
    "@gomflow/shared": "file:../gomflow-shared",
    "@supabase/supabase-js": "^2.45.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "discord.js": "^14.14.1",
    "axios": "^1.6.0",
    "bull": "^4.12.0",
    "ioredis": "^5.3.2",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "form-data": "^4.0.0",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/compression": "^1.7.5",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.11",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.2",
    "tsx": "^4.6.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "gomflow",
    "discord",
    "bot",
    "slash-commands",
    "group-orders",
    "messaging",
    "automation",
    "embeds"
  ],
  "author": "GOMFLOW Team",
  "license": "MIT"
}